CC=@CC@
RANLIB=@RANLIB@
AR=@AR@
RM=@RM@
CFLAGS=@CFLAGS@ @OPTIM@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@ $(LIBS)
INCDIR=../include
INCLUDES=-I$(INCDIR)
MKDEP=../helpers/mkdep.sh

OBJS=apr_cpystrn.o \
	apr_snprintf.o \
	apr_strnatcmp.o \
	apr_strings.o \
	apr_fnmatch.o

.c.o:
	$(CC) $(CFLAGS) -c $(INCLUDES) $<

all: $(OBJS)

clean:
	$(RM) -f *.o *.a *.so

distclean: clean
	-$(RM) -f Makefile

depend:
	$(MKDEP) $(INCLUDES) $(CFLAGS) *.c

# DO NOT REMOVE
