CC=@CC@
RANLIB=@RANLIB@
AR=@AR@
RM=@RM@
CFLAGS=@CFLAGS@ @OPTIM@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@ $(LIBS)
INCDIR=../include
INCLUDES=-I$(INCDIR) -I$(INCDIR)/arch/@OSDIR@
MKDEP=../helpers/mkdep.sh

OBJS=apr_pools.o

.c.o:
	$(CC) $(CFLAGS) -c $(INCLUDES) $<

all: $(OBJS)

clean:
	$(RM) -f *.o *.a *.so

distclean: clean
	-$(RM) -f Makefile

depend:
	$(MKDEP) $(INCLUDES) $(CFLAGS) *.c

# DO NOT REMOVE
