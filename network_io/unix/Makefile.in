RM=@RM@
CC=@CC@
RANLIB=@RANLIB@
CFLAGS=@CFLAGS@ @OPTIM@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@ $(LIBS)
INCDIR=../../include
OSDIR=$(INCDIR)/arch/@OSDIR@
DEFOSDIR=$(INCDIR)/arch/@DEFAULT_OSDIR@
INCLUDES=-I$(INCDIR) -I$(OSDIR) -I$(DEFOSDIR)
MKDEP=../../helpers/mkdep.sh

LIB=libnetwork.a

OBJS=poll.o \
	sendrecv.o \
	sockets.o \
	sockopt.o \
	sockaddr.o

.c.o:
	$(CC) $(CFLAGS) -c $(INCLUDES) $<

all: $(OBJS)

clean:
	$(RM) -f *.o *.a *.so

distclean: clean
	-$(RM) -f Makefile

depend:
	$(MKDEP) $(INCLUDES) $(CFLAGS) *.c

# DO NOT REMOVE
