CC=@CC@
RANLIB=@RANLIB@
CFLAGS=@CFLAGS@ @OPTIM@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@ $(LIBS)
INCDIR=../../include
INCLUDES=-I$(INCDIR) -I$(INCDIR)/arch
MKDEP=../../helpers/mkdep.sh

LIB=liblock.a

OBJS=locks.o \
	crossproc.o \
	intraproc.o \

.c.o:
	$(CC) $(CFLAGS) -c $(INCLUDES) $<

all: $(LIB)

clean:
	$(RM) -f *.o *.a *.so

distclean: clean
	-$(RM) -f Makefile


$(LIB): $(OBJS)
	$(RM) -f $@
	$(AR) cr $@ $(OBJS)
	$(RANLIB) $@

depend:
	$(MKDEP) $(INCLUDES) $(CFLAGS) *.c

# DO NOT REMOVE
